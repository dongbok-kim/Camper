<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.3.xsd">
		
	<mvc:interceptors>
		<mvc:interceptor>
			<mvc:mapping path="/myBlockList.go"/>
			<mvc:mapping path="/myBlockDelete.do"/>
			<mvc:mapping path="/myCampingLikeList.go"/>
			<mvc:mapping path="/myCampingLikeDelete.do"/>
			<mvc:mapping path="/myCrewList.go"/>
			<mvc:mapping path="/crewUpdate.go"/>
			<mvc:mapping path="/crewUpdate.do"/>
			<mvc:mapping path="/myInquiryList.go"/>
			<mvc:mapping path="/inquiryWrite.go"/>
			<mvc:mapping path="/inquiryWrite.do"/>
			<mvc:mapping path="/inquiryDetail.go"/>
			<mvc:mapping path="/myInquiryDelete.do"/>
			<mvc:mapping path="/myInquiryUpdate.go"/>
			<mvc:mapping path="/myInquiryUpdate.do"/>
			<mvc:mapping path="/campingReviewForm.go"/>
			<mvc:mapping path="/campingReviewWrite.do"/>
			<mvc:mapping path="/myCampingReview.go"/>
			<mvc:mapping path="/campingReviewDelete.do"/>
			<mvc:mapping path="/crewReviewForm.go"/>
			<mvc:mapping path="/crewReview.do"/>
			<mvc:mapping path="/myCrewReviewR.go"/>
			<mvc:mapping path="/myCrewReviewW.go"/>
			<mvc:mapping path="/crewReviewDelete.do"/>
			<mvc:mapping path="/myCrewWriteList.go"/>
			<mvc:mapping path="/myCrewWriteDelete.do"/>
			<bean class="com.camper.lib.utils.MypgValidationInter"/>
		</mvc:interceptor>
		<mvc:interceptor>
			<mvc:mapping path="/관리자요청명모두나열1"/>
			<mvc:mapping path="/관리자요청명모두나열2"/>
			<mvc:mapping path="/관리자요청명모두나열3"/>
			<bean class="com.camper.lib.utils.AdminValidationInter"/>
		</mvc:interceptor>
	</mvc:interceptors>

</beans>
